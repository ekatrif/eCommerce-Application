{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
  },
  "plugins": [{ "name": "typescript-plugin-css-modules" }],
  "include": ["./src/**/*"],
  "typeRoots": [
    "./node_modules/@types",
  ]     
}