{
  "compilerOptions": {
    "target": "es2017",
    "module": "ES2022",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",  
  },
  "plugins": [{ "name": "typescript-plugin-css-modules" }],
  "include": ["./src/**/*"],
  "typeRoots": [
    "./node_modules/@types",
  ],   
}